                            const product_id = element.dataset.productId;
                            const variants = JSON.parse(rootObject.dataset.variants);
                            const optionsSize = parseInt(element.dataset.optionsSize);
                            let selectedOptions = '';
                            selectedOptions += element.dataset.value;
                            let variantObject = {};
                            for(let j=0; j<variants.length; j++){
                                const variant = variants[j];
                                if(variant.options === selectedOptions){
