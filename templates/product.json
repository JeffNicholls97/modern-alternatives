    "box_usp_CWkV3z": {
      "type": "box_usp",
      "disabled": true,
      "settings": {
        "usp_one_title": "Prepare for Anything",
        "usp_one": "From hurricanes to blackouts, reliable solar power ensures you're able to keep your home & family safe no matter what comes your way.",
