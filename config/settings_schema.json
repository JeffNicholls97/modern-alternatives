        "id": "roi_calc_heading",
        "label": "Heading",
        "default": "Solar ROI Calculator"
      },
      {
        "type": "inline_richtext",
        "id": "roi_calc_price_label",
        "label": "Price Label",
        "default": "<span><strong>Total Solar Kit Price<\/strong> (Editable)<\/span>"
      },
      {
        "type": "number",
        "id": "roi_calc_default_price",
        "label": "Default Total Kit Price",
        "default": 10000
      },
      {
        "type": "header",
        "content": "Calculations Settings",
        "info": "Use vars that are defined below to create calculations"
      },
      {
        "type": "paragraph",
        "content": "[price]"
      },
      {
        "type": "paragraph",
        "content": "[roi_result_federal_tax_credit]"
      },
      {
        "type": "paragraph",
        "content": "[roi_result_estimated_annual_electricity_savings]"
      },
      {
        "type": "inline_richtext",
        "id": "roi_result_federal_tax_credit_label",
        "label": "Federal Tax Credit Label",
        "default": "<span>30% Federal Tax Credit<\/span>"
      },
      {
        "type": "text",
        "id": "roi_result_federal_tax_credit_calculations",
        "label": "Federal Tax Credit Calculations",
        "default": "[price]*0.3"
      },
      {
        "type": "inline_richtext",
        "id": "roi_result_estimated_annual_electricity_savings_label",
        "label": "Estimated Annual Electricity Savings Label",
        "default": "<span>Estimated <strong><u>Annual<\/u><\/strong> Electricity Savings<\/span>"
      },
      {
        "type": "text",
        "id": "roi_result_estimated_annual_electricity_savings_calculations",
        "label": "Estimated Annual Electricity Savings Calculations",
        "default": "[price]*0.09"
      },
      {
        "type": "inline_richtext",
        "id": "roi_result_cost_after_incentives_and_savings_label",
        "label": "Cost After Incentives & Savings Label",
        "default": "<span><strong>Cost After Incentives & Savings<\/strong><\/span>"
      },
      {
        "type": "text",
        "id": "roi_result_cost_after_incentives_and_savings_calculations",
        "label": "Cost After Incentives & Savings Calculations",
        "default": "[price]-[roi_result_federal_tax_credit]-[roi_result_estimated_annual_electricity_savings]"
      },
      {
        "type": "inline_richtext",
        "id": "roi_result_estimated_home_value_increase_label",
        "label": "Estimated Home Value Increase Label",
        "default": "<span>Estimated Home Value Increase<\/span>"
      },
      {
        "type": "text",
        "id": "roi_result_estimated_home_value_increase_calculations",
        "label": "Estimated Home Value Increase Calculations",
        "default": "[price]+0.4*[price]"
      },
      {
        "type": "richtext",
        "id": "roi_calc_additional_text",
        "label": "Additional text",
        "default": "<p>These are estimates based on the national averages.<\/p><p>Please use these as strictly estimates and not exact amounts.<\/p>"
      }
    ]
